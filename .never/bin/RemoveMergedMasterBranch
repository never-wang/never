#########################################################################
remote_branches=`git branch --merged origin/master -r | grep -v HEAD | grep -v develop | grep -v master | sed s/origin\\\\///g`

if [ -n "$remote_branches" ]; then
    git push origin --delete $remote_branches
fi

git branch --merged master

local_branch=`git branch --merged master | grep -v develop | grep -v master | sed s/origin\\\\///g`

if [ -n "$local_branches" ]; then
    git branch -d $local_branches
fi
