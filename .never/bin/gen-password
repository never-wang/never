import random
import string
import sys

length = 8
if (len(sys.argv) > 1):
    length = int(sys.argv[1])

print ''.join(random.sample("0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ!\"#$%&\'()*+,-./:;?@[\\]^_`{|}~", length))
